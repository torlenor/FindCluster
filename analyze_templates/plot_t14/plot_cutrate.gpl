set term pdfcairo enhan

load "./gnuplot.settings"

set output sprintf("cutrate_f%.2f.pdf", f)

set label 1 sprintf("Cut f=%.2f", f) at 5.5,0.8

set xrange [Trangemin:Trangemax]
set yrange [-0.05:1.05]

set xlabel "T/T_c"
set ylabel "Fraction of points cut"

set pointsize 0.4
linew=3

plot "./cutrate.res" u (Ntcrit/$1):2:3 w yerrorlines lw linew notitle
